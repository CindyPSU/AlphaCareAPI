<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.PatientListXMLController">
    <Label text="Filter" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TextField fx:id="searchBar" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.hgrow="ALWAYS"/>
    <TableView fx:id="tableView" 
        GridPane.columnIndex="0" 
        GridPane.columnSpan="2"
        GridPane.rowIndex="1"
        GridPane.hgrow="ALWAYS" 
        GridPane.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="tableViewColumn" text="Patients"></TableColumn>
        </columns>
    </TableView>
</GridPane>
